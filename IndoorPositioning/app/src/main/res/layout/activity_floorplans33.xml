<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">
<!--
    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

        -->
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/button_floor0"
        android:text="nothing" />

    <TextView
        android:id="@+id/xtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50px"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:text="x:"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="60dp"
        android:alpha="0"/>

    <TextView
        android:id="@+id/ytext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50px"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:text="y:"
        android:layout_marginBottom="30dp"
        android:alpha="0"/>

    <TextView
        android:alpha="0"
        android:id="@+id/ztext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50px"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:text="z:" />


    <Button
        android:textSize="14dp"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:text="Outline"
        android:id="@+id/button_floor0"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_alignTop="@+id/xtext" />

    <ListView
        android:id="@+id/listView1"
        android:layout_width="400dp"
        android:layout_height="200dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:drawSelectorOnTop="true"
        android:scrollbars="vertical"
        android:listSelector="@android:color/darker_gray" >

    </ListView>

    <ImageView
        android:id="@+id/wallsView33"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/outline35">
    </ImageView>
<!--
    <ImageView
        android:layout_width="40px"
        android:layout_height="40px"
        android:id="@+id/arrowView"
        android:src="@drawable/arrow_top_right"
        android:layout_marginLeft="200px"
        android:layout_marginTop="200px"/>
-->

    <GridView
        android:id="@+id/mapGrid33"
        android:layout_alignParentTop="true"
        android:layout_marginTop="208dp"
        android:layout_marginLeft="240dp"
        android:layout_marginRight="180dp"
        android:numColumns="14"
        android:gravity="center"
        android:columnWidth="102px"
        android:stretchMode="none"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"/>



</RelativeLayout>
